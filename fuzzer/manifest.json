{
  "manifest_version": 2,

  "name": "Fuzzerrify",
  "description": "DESCRIPTION",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
     "*://*/*"
  ],
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["jquerymin.js", "actionScript.js"]
    }
  ]
}
